{
  "ecmaFeatures": {
    "es6": true,
    "jsx": false,
    "modules": false
  },
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": [
    "./node_modules/eslint-config-gobold/.eslintrc"
  ],
  "globals": {
    "$": true,
    "UIkit": true,
    "chai": true,
    "angular": true,
    "_": true,
    "moment": true
  },
  "rules": {
    "camelcase": 0,
    "comma-spacing": 1,
    "consistent-this": 1,
    "curly": 1,
    "default-case": 1,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "no-redeclare": 1,
    "no-shadow": 1,
    "no-trailing-spaces": 1,
    "no-undef": 1,
    "no-unreachable": 1,
    "no-use-before-define": 1,
    "operator-assignment": 1,
    "quotes": 1,
    "radix": 1,
    "semi": 1,
    "space-unary-ops": 1,
    "strict": 0
  }
}
